# CDN Configuration
NEXT_PUBLIC_CDN_URL=https://cdn.example.com
NEXT_PUBLIC_FALLBACK_URL=https://fallback.example.com

# Performance Optimization
NEXT_PUBLIC_ENABLE_CDN=true
NEXT_PUBLIC_IMAGE_QUALITY=80
NEXT_PUBLIC_ENABLE_WEBP=true
NEXT_PUBLIC_ENABLE_AVIF=true

# Analytics
NEXT_PUBLIC_GA_ID=your-google-analytics-id
NEXT_PUBLIC_GTM_ID=your-google-tag-manager-id

# Performance Monitoring
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_PERFORMANCE_SAMPLE_RATE=0.1
