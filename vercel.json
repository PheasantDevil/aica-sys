{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXTAUTH_URL": "https://aica-sys.vercel.app",
    "NEXT_PUBLIC_BASE_URL": "https://aica-sys.vercel.app",
    "NEXT_PUBLIC_API_URL": "https://aica-sys-backend.vercel.app"
  },
  "functions": {
    "frontend/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
